-- // LOGIC BARISTA
-- =========================
-- SERVICES
-- =========================
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local TweenService = game:GetService("TweenService")

local LP = Players.LocalPlayer
local PlayerGui = LP:WaitForChild("PlayerGui")

-- =========================
-- STATE
-- =========================
_G.AutoFarmBarista = _G.AutoFarmBarista or false
local BaristaRunning = false
local isWorking = false

-- =========================
-- PATHS & REMOTES
-- =========================
local BaristaJob = workspace:WaitForChild("BaristaJob")
local Interactions = BaristaJob:WaitForChild("Interactions")
local JobEvents = ReplicatedStorage:WaitForChild("JobEvents")
local TeamChangeRequest = JobEvents:WaitForChild("TeamChangeRequest")

local BaristaAssets = ReplicatedStorage:WaitForChild("BaristaAssets")
local BaristaEvent = BaristaAssets:WaitForChild("Events"):WaitForChild("BaristaEvent")

local startPart = Interactions.StartPart.StartPart
local machinePart = Interactions.MachinePart.MachinePart
local registerPart = Interactions.RegisterPart.RegisterPart
local supplyPart = Interactions.SupplyPart.SupplyPart

-- COORDINATES TRANSITION
local POS_EXIT_BARISTA = Vector3.new(-5004.42, 2.89, -678.09)
local POS_SUPPLY = Vector3.new(-5114.04, 2.89, -669.93)

-- =========================
-- GET HRP
-- =========================
local function getHRP()
    local char = LP.Character or LP.CharacterAdded:Wait()
    return char:WaitForChild("HumanoidRootPart")
end

-- =========================
-- SAFE TWEEN ENGINE
-- =========================
local function safeTween(targetCF, speed)
    local hrp = getHRP()
    local VOID_Y = -25
    local OFFSET = 2
    local SPEED = speed or 140

    local downCF = CFrame.new(hrp.Position.X, VOID_Y, hrp.Position.Z)
    TweenService:Create(hrp, TweenInfo.new(0.4, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {CFrame = downCF}):Play()
    task.wait(0.45)

    local targetVoid = CFrame.new(targetCF.X, VOID_Y, targetCF.Z)
    local dist = (hrp.Position - targetVoid.Position).Magnitude
    local moveTween = TweenService:Create(hrp, TweenInfo.new(dist / SPEED, Enum.EasingStyle.Linear), {CFrame = targetVoid})
    moveTween:Play()
    moveTween.Completed:Wait()

    TweenService:Create(hrp, TweenInfo.new(0.4, Enum.EasingStyle.Sine, Enum.EasingDirection.In), {CFrame = targetCF + Vector3.new(0, OFFSET, 0)}):Play()
    task.wait(0.45)
end

-- =========================
-- SMART CLICK
-- =========================
local function smartWaitAndClick(prompt, holdTime)
    if not prompt then return end
    local t = 0
    while (not prompt.Enabled or not prompt.Parent) and t < 50 do
        task.wait(0.1)
        t += 1
    end
    task.wait(0.6)
    fireproximityprompt(prompt)
    if holdTime then task.wait(holdTime) end
end

-- =========================
-- REPAIR LOGIC (2 TAHAP)
-- =========================
local function runRepairFlow()
    isWorking = true
    print("üõ†Ô∏è Mesin Merah! OTW Supply...")
    safeTween(CFrame.new(POS_EXIT_BARISTA))
    safeTween(CFrame.new(POS_SUPPLY))
    
    BaristaEvent:FireServer("RestockSupplies")
    local prompt = supplyPart:FindFirstChild("SupplyPrompt")
    smartWaitAndClick(prompt, 6)
    
    safeTween(CFrame.new(POS_EXIT_BARISTA))
    isWorking = false
end

-- =========================
-- STEALTH MINIGAME & REGISTER
-- =========================
local function applyStealthBypass()
    local gui = PlayerGui:FindFirstChild("BaristaGUI")
    local mini = gui and gui:FindFirstChild("MinigameFrame")
    if not mini or not mini.Visible or isWorking then return end

    isWorking = true
    pcall(function()
        local bg = mini:WaitForChild("BackgroundBar")
        local target = bg:WaitForChild("TargetZone")
        target.Size = UDim2.new(1.8,0,1.8,0)
        target.Position = UDim2.new(-0.4,0,-0.4,0)
        target.BackgroundTransparency = 1
    end)

    while mini.Visible and _G.AutoFarmBarista do task.wait(0.5) end
    
    safeTween(registerPart.CFrame)
    smartWaitAndClick(registerPart:FindFirstChild("RegisterPrompt"))
    task.wait(1)
    
    safeTween(machinePart.CFrame)
    isWorking = false
end

-- =========================
-- MAIN BARISTA CONTROLLER
-- =========================
local function StartBaristaGhost()
    if BaristaRunning then return end
    BaristaRunning = true

    task.spawn(function()
        -- Request Team
        TeamChangeRequest:FireServer("Barista", 11378976, 1, 0, "Detector")
        task.wait(1)

        while _G.AutoFarmBarista do
            local missionUI = PlayerGui:FindFirstChild("BaristaMissionUI")
            
            -- PRIORITY 1: AMBIL JOB (HARUS PALING UTAMA)
            if not missionUI and not isWorking then
                print("üìù Job not found. Taking job first...")
                safeTween(startPart.CFrame)
                smartWaitAndClick(startPart:WaitForChild("JobPrompt"))
                
                -- Tunggu sampai UI muncul sebelum lanjut ke logic lain
                local waitTime = 0
                while not PlayerGui:FindFirstChild("BaristaMissionUI") and waitTime < 40 do
                    task.wait(0.2)
                    waitTime += 1
                end
                print("‚úÖ Job Registered!")
                
                -- Langsung Tween ke Machine setelah ambil job
                if _G.AutoFarmBarista then
                    safeTween(machinePart.CFrame)
                end

            -- PRIORITY 2: REPAIR (MESIN MERAH)
            elseif missionUI then
                local label = missionUI:FindFirstChild("ObjectiveLabel", true)
                if label and label.TextColor3 == Color3.fromRGB(255, 50, 50) and not isWorking then
                    runRepairFlow()
                end
            end

            -- PRIORITY 3: MINIGAME & WORK
            if missionUI and not isWorking then
                local gui = PlayerGui:FindFirstChild("BaristaGUI")
                local mini = gui and gui:FindFirstChild("MinigameFrame")

                if mini and mini.Visible then
                    applyStealthBypass()
                else
                    local hrp = getHRP()
                    if (hrp.Position - machinePart.Position).Magnitude > 15 then
                        safeTween(machinePart.CFrame)
                    else
                        local prompt = machinePart:FindFirstChild("MachinePrompt")
                        if prompt and prompt.Enabled then
                            smartWaitAndClick(prompt)
                        end
                    end
                end
            end

            task.wait(1)
        end
        BaristaRunning = false
    end)
end

-- =========================
-- TOGGLE WATCHER
-- =========================
task.spawn(function()
    while true do
        task.wait(0.5)
        if _G.AutoFarmBarista then
            StartBaristaGhost()
        end
    end
end)


-- =========================
-- SYSTEM LOOPS (FPS Counter)
-- =========================
local lastClock = os.clock()
local fpsCounter = 0
local fps = 0

RunService.Heartbeat:Connect(function()
fpsCounter += 1
if os.clock() - lastClock >= 1 then
fps = fpsCounter
fpsCounter = 0
lastClock = os.clock()
end
end)

-- =========================
-- FPS / PING LOOP (FIXED)
-- =========================
task.spawn(function()
    local fpsCounter = 0
    local currentFps = 0

    RunService.Heartbeat:Connect(function()
        fpsCounter += 1
    end)

    while true do
        task.wait(1)

        currentFps = fpsCounter
        fpsCounter = 0

        -- Update FPS
        if FpsLabel then
            FpsLabel:SetDesc(tostring(currentFps) .. " FPS")
        end

        -- Update Ping
        local ping = math.floor(LP:GetNetworkPing() * 1000)
        if PingLabel then
            PingLabel:SetDesc(tostring(ping) .. " ms")
        end
    end
end)
-- // HIDE NAME & CHARACTER LOOP (FIXED)
RunService.Heartbeat:Connect(function()
local char = LP.Character
if not char then return end

-- HIDE NAME
if _G.HideNameEnabled then
local head = char:FindFirstChild("Head")
local tag = head and head:FindFirstChildOfClass("BillboardGui")
if tag then
tag.Enabled = false
end
end

-- HIDE CHARACTER
if _G.HideCharEnabled then
for _, v in pairs(char:GetDescendants()) do
if v:IsA("BasePart") then
v.Transparency = 1
v.CanCollide = false
elseif v:IsA("Decal") then
v.Transparency = 1
end
end
end

end)

-- ANTI AFK
local Players = game:GetService("Players")
local VirtualUser = game:GetService("VirtualUser")
local LP = Players.LocalPlayer

_G.AntiAFK = true

task.spawn(function()
while true do
task.wait(60) -- tiap 60 detik
if _G.AntiAFK and LP.Character then
VirtualUser:CaptureController()
VirtualUser:Button1Down(Vector2.new(0,0))
task.wait(0.1)
VirtualUser:Button1Up(Vector2.new(0,0))
VirtualUser:Button2Down(Vector2.new(0,0))
task.wait(0.1)
VirtualUser:Button2Up(Vector2.new(0,0))
end
end
end)

