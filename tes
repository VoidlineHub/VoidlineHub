HomeTab:Section({ Title = "Game Information" })

-- Bikin objeknya dulu biar variabelnya ADA
local CurrentMoney = HomeTab:Button({ Title = "Money", Desc = "Rp0", Icon = "banknote" })
local FpsLabel = HomeTab:Button({ Title = "FPS", Desc = "0 FPS", Icon = "monitor" })
local PingLabel = HomeTab:Button({ Title = "Ping", Desc = "0 ms", Icon = "signal" })
local GameTime = HomeTab:Button({ Title = "Uptime", Desc = "00:00:00", Icon = "clock" })

-- LOGIC UPDATE STATS (FPS, PING, UPTIME)
local fpsTick = 0
RunService.Heartbeat:Connect(function() fpsTick = fpsTick + 1 end)
local StartTime = os.time()

task.spawn(function()
    local PlayerData = LP:WaitForChild("PlayerData", 10)
    local MoneyValue = PlayerData and PlayerData:WaitForChild("RPValue", 10)

    while true do
        local currentFps = fpsTick
        fpsTick = 0
        local ping = math.floor(LP:GetNetworkPing() * 1000)
        local runtime = os.time() - StartTime
        
        -- Update UI
        FpsLabel:SetDesc(currentFps .. " FPS")
        PingLabel:SetDesc(ping .. " ms")
        
        local h = math.floor(runtime/3600)
        local m = math.floor((runtime%3600)/60)
        local s = runtime%60
        GameTime:SetDesc(string.format("%02d:%02d:%02d", h, m, s))
        
        if MoneyValue then
            CurrentMoney:SetDesc(FormatRupiah(MoneyValue.Value))
        end
        
        task.wait(1)
    end
end)

-- ============ AUTO FARM TAB ===========
local AutoFarmTab = Window:Tab({

        Title = "Auto Farm",

        Icon = "solar:cpu-bold",

        Color = primaryColor

    })

AutoFarmTab:Section({ Title = "Bandung Farm" })

local BandungToggle = AutoFarmTab:Toggle({
    Title = "Auto Farm Bandung",
    Callback = function(v)
        if v then
            local motor = LP.Character 
                and LP.Character:FindFirstChild("Humanoid")
                and LP.Character.Humanoid.SeatPart

            if not motor then
                WindUI:Notify({
                    Title = "Error",
                    Content = "Duduk di mobil dulu!",
                    Duration = 3
                })
                BandungToggle:Set(false) -- matikan toggle lagi
                return
            end

            SetBlackScreen(true)
            StartRoute()
        else
            StopRoute() -- sudah matikan blackscreen di dalam
        end
    end
})

AutoFarmTab:Section({ Title = "courier farm" })

local CourierToggle = AutoFarmTab:Toggle({
    Title = "Auto Farm courier",
    Locked = true,
    LockedTitle = "still in development",
    Callback = function(v) end
})

