-- (Global Status Tetap Ada)
_G.Status = _G.Status or { Courier = "Waiting", Office = "Waiting", Pause = "Disabled" }

local function FormatRupiah(num)
    local s = tostring(num)
    local result = s:reverse():gsub("(%d%d%d)", "%1."):reverse()
    return "Rp" .. (result:sub(1,1) == "." and result:sub(2) or result)
end

-- ================================
-- ========= BLACK SCREEN =========
-- ================================

local BlackScreenGui = nil
local function SetBlackScreen(state)
    if state then
        if not BlackScreenGui then
            BlackScreenGui = Instance.new("ScreenGui", CoreGui)
            BlackScreenGui.Name = "BilzzBlackScreen"
            BlackScreenGui.IgnoreGuiInset = true
            BlackScreenGui.DisplayOrder = -1

            local Frame = Instance.new("Frame", BlackScreenGui)
            Frame.Size = UDim2.new(1, 0, 1, 0)
            Frame.BackgroundColor3 = Color3.new(0, 0, 0)
            Frame.BorderSizePixel = 0

            local Label = Instance.new("TextLabel", Frame)
            Label.Size = UDim2.new(1, 0, 1, 0)
            Label.BackgroundTransparency = 1
            Label.Text = "AUTOFARM ON\nVoidLineHub"
            Label.TextColor3 = Color3.new(1, 1, 1) -- Tetap Putih
            Label.Font = Enum.Font.GothamBold
            Label.TextScaled = false
            Label.TextSize = 22

            -- Efek Kedap-Kedip (Blink)
            task.spawn(function()
                while BlackScreenGui do
                    -- Teks menghilang perlahan (Fade out)
                    local tweenOut = TweenService:Create(Label, TweenInfo.new(0.8), {TextTransparency = 1})
                    tweenOut:Play()
                    tweenOut.Completed:Wait()
                    
                    if not BlackScreenGui then break end
                    
                    -- Teks muncul lagi (Fade in)
                    local tweenIn = TweenService:Create(Label, TweenInfo.new(0.8), {TextTransparency = 0})
                    tweenIn:Play()
                    tweenIn.Completed:Wait()
                    
                    task.wait(0.2) -- Jeda sebentar sebelum kedip lagi
                end
            end)
        end
    else
        if BlackScreenGui then
            BlackScreenGui:Destroy()
            BlackScreenGui = nil
        end
    end
end

-- ================================
-- ========= FORMAT RUPIAH ========
-- ================================

local function FormatRupiah(num)
    local s = tostring(num)
    local result = s:reverse():gsub("(%d%d%d)", "%1."):reverse()
    if result:sub(1,1) == "." then
        result = result:sub(2)
    end
    return "Rp" .. result
end

-- autofarm vehicle 
-- [[ CONFIG ]] --
local MaxSpeed = 270
local targets = {
    [1] = Vector3.new(-2734.138, 82.916, -10328.666),
    [2] = Vector3.new(679.88, 113.83, -10349.24),
    [3] = Vector3.new(1151.91, 139.1, -10501.67),
    [4] = Vector3.new(1582.02, 163.23, -10781.39),
    [5] = Vector3.new(1862.21, 180.58, -11171.73),
    [6] = Vector3.new(2005.09, 192.32, -11741.17),
    [7] = Vector3.new(1988.58, 82.52, -13713.74),
    [8] = Vector3.new(1841.28, 113.36, -16158.04),
    [9] = Vector3.new(1671.27, 112.27, -17283.7),
    [10] = Vector3.new(1084.64, 86.2, -19560.86),
    [11] = Vector3.new(619.49, 81.97, -21836.02),
    [12] = Vector3.new(548.91, 107.44, -22433.84),
    [13] = Vector3.new(355.634, 213.801, -25856.560)
}

local AutoFarm = false
local DirectionForward = true 
local RoadName = "PrivateHighway_BillzzHub"

-- [[ JALAN TOL GENERATOR ]] --
local function CreateHighway()
    local startPos = targets[1]
    local endPos = targets[13]
    local distance = (startPos - endPos).Magnitude
    
    local Road = Instance.new("Part")
    Road.Name = RoadName
    Road.Size = Vector3.new(50, 1, distance + 100) -- Lebar 50, panjang ngikutin rute
    Road.CFrame = CFrame.lookAt(startPos, endPos) * CFrame.new(0, -3.5, -distance/2)
    Road.Anchored = true
    Road.Transparency = 0.7 -- Kasih transparan dikit biar kelihatan jalannya
    Road.Color = Color3.fromRGB(0, 255, 255) -- Warna neon biar keren
    Road.Material = Enum.Material.Neon
    Road.Parent = workspace
end

local function RemoveHighway()
    local road = workspace:FindFirstChild(RoadName)
    if road then road:Destroy() end
end

local function GetVehicle()
    local char = LP.Character
    if char and char:FindFirstChild("Humanoid") and char.Humanoid.Sit then
        return char.Humanoid.SeatPart and char.Humanoid.SeatPart.Parent
    end
    return nil
end

local function StartRoute()
    local motor = GetVehicle()
    local char = LP.Character
    if not motor or not char then return end
    local primary = motor.PrimaryPart or motor:FindFirstChildOfClass("BasePart")
    local hum = char:FindFirstChildOfClass("Humanoid")
    
    AutoFarm = true
    CreateHighway() -- Pasang jalan pas start

    while AutoFarm do
        local currentPath = {}
        if DirectionForward then
            for i = 1, #targets do table.insert(currentPath, targets[i]) end
        else
            for i = #targets, 1, -1 do table.insert(currentPath, targets[i]) end
        end

        for i, targetPos in ipairs(currentPath) do
            if not AutoFarm then break end
            
            local distance = (primary.Position - targetPos).Magnitude
            
            while distance > 6 and AutoFarm do
                RunService.Heartbeat:Wait()
                distance = (primary.Position - targetPos).Magnitude
                
                -- [[ ANTI RAGDOLL ]] --
                hum:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
                hum:SetStateEnabled(Enum.HumanoidStateType.FallingDown, false)
                
                local currentSpeed = MaxSpeed
                
                -- [[ LOGIC PENGEREMAN ]] --
                if i == #currentPath then
                    if distance < 180 then
                        currentSpeed = math.clamp((distance / 180) * MaxSpeed, 5, MaxSpeed)
                    end
                end

                -- Movement
                if currentSpeed > 0 then
                    primary.AssemblyLinearVelocity = (targetPos - primary.Position).Unit * currentSpeed
                    primary.CFrame = CFrame.lookAt(primary.Position, targetPos)
                else
                    primary.AssemblyLinearVelocity = Vector3.zero
                end
                
                primary.AssemblyAngularVelocity = Vector3.zero

                -- [[ SMART NOCLIP ]] --
                -- Tembus semua kecuali Jalan Tol kita
                for _, v in pairs(char:GetDescendants()) do 
                    if v:IsA("BasePart") then v.CanCollide = false end 
                end
                for _, v in pairs(motor:GetDescendants()) do 
                    if v:IsA("BasePart") then 
                        -- Biar PrimaryPart/Ban tetep napak di Jalan Tol
                        if v.Name == primary.Name then
                            v.CanCollide = true
                        else
                            v.CanCollide = false 
                        end
                    end 
                end
            end
        end

        -- [[ STOPPER & U-TURN ]] --
        if AutoFarm then
            primary.AssemblyLinearVelocity = Vector3.zero
            task.wait(1.5)
            
            -- U-Turn Smooth
            local targetRotation = primary.CFrame * CFrame.Angles(0, math.rad(180), 0)
            TweenService:Create(primary, TweenInfo.new(1.2), {CFrame = targetRotation}):Play()
            task.wait(1.3)
            
            DirectionForward = not DirectionForward
        end
    end
end

local function StopRoute()
    AutoFarm = false
    RemoveHighway()
    SetBlackScreen(false) -- ‚Üê tambahin ini biar sinkron sama toggle
    print("‚ùå autofarm stop.")
end

